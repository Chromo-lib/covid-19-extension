{
  "name": "CovidNow",
  "version": "1.0.3",
  "description": "CovidNow allows you to track the world-wide spread of Corona virus.",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "icons": {
    "256": "icons/icon256.png",
    "128": "icons/icon128.png",
    "64": "icons/icon64.png",
    "32": "icons/icon32.png"
  },
  "browser_action": {
    "default_title": "CovidNow",
    "default_popup": "popup.html"
  }
}