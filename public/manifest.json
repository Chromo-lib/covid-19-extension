{
  "name": "CovidNow",
  "version": "1.0.7",
  "description": "CovidNow allows you to follow the global spread of the Corona virus.",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "icons": {
    "256": "icons/icon256.png",
    "128": "icons/icon128.png",
    "64": "icons/icon64.png",
    "32": "icons/icon32.png"
  },
  "browser_action": {
    "default_title": "CovidNow",
    "default_popup": "index.html",
    "default_icon": "icons/icon32.png"
  }
}